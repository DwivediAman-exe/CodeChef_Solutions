#include <bits/stdc++.h> 
using namespace std; 
typedef long double ld;
typedef long long int ll;
typedef unsigned long long int ull;
const long long int m=pow(10,9)+7;

int main() 
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    ll t;
    cin>>t;
    while(t--)
    {
        ll m,h;
        cin>>m>>h;
        ll b=m/(h*h);
        if(b<=18)
            cout<<"1"<<endl;
        else if(b>18&&b<=24)
            cout<<"2"<<endl;
        else if(b>24&&b<=29)
            cout<<"3"<<endl;
        else 
            cout<<"4"<<endl;
            
    }
    return 0;
}
